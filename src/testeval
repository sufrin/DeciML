---+ Standard library notation



notation id ~ ϕ Π ∐ ∀ ∃ ↯ ;;

notation
     left  6 && ||      ;
     left  7 + -        ;
     left  8 × * /      ;
     right 9 **         ;;

let flip f x y = f y x;;

let x + y = prim_add x y;
    (-) = prim_sub;
    (*) = prim_mul;
    (/) = prim_div;
    (×) = prim_mul;
    (=) = prim_eq;;


